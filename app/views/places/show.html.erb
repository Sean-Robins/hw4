<h1><%= @place["name"] %></h1>
<h3>My Entries</h3>
<p></p>
<div>
  <a href="/entries/new?place_id=<%= @place["id"] %>" class="btn btn-info">New Entry</a>
  <a href="/places" class="btn btn-light">Back to All Places</a>
</div>
<p></p>

<div class="row">  
    <% for entry in @entries %>
      <div class="col-sm-6 col-md-4 col-lg-3">
        <h4><%= entry["title"] %></h4>
        <h6><%= entry["occurred_on"] %></h6>
        <p> <img src="<%= url_for(entry.uploaded_image) %>" class="img-fluid"></p>
        <p><%= entry["description"] %></p>
      </div>
    <% end %>   
</div>